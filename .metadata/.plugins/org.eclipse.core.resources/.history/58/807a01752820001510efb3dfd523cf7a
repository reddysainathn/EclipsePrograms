package Arrays;

public class MultiDimensionalArrays {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//One-Dimentional Array
		int[] values = {1,3,2343};
		
		System.out.println(values[2]);
		
		//Two-Dimentional Array

		int[][] grid = { 
				{1,3,2343},
				{2,4},
				{1,2,3,4}
				};
		System.out.println(grid[1][1]);
		}
	}


