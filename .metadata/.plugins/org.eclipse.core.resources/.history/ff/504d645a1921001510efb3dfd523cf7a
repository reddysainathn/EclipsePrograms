package ClassesandObjects;

public class Plant {
	// Bad Practice
	public String name;
	// Acceptable Practice --- It's Final.
	public static final int ID = 8;

	// private is only upto the class
	private String type;

	public Plant() {
		// TODO Auto-generated constructor stub
		this.name = "Freddy";

		type = "plant";
	}
}
