/*
 * ______________
   |     A       |
   |       int a |
   |_____________|
         |   
         |
         |
   --------------
   |    B        |
   |      int b  |
   |_____________|
 */
 class A{
	private int a;
	public A(int a){
		this.a=a;
		
	}
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public boolean zero(){
	return 	a==0;
	}
	public int oneMore(){
		return a++;
	}
}
    
public class Example1 {
	
public static void main(String[] args) {
	
}
}
