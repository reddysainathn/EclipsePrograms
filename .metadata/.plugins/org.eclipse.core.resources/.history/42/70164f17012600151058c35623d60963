import java.util.ArrayList;

class Machine {

	@Override
	public String toString() {
		return "I am a Machine";
	}

}

class Camera extends Machine {
	@Override
	public String toString() {
		return "I am a Camera";
	}

}

public class GenericsAndWildCards {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		ArrayList<Machine> machine = new ArrayList<Machine>();
		machine.add(new Machine());
		machine.add(new Machine());
		showMachine(machine);

		ArrayList<Machine> machine1 = new ArrayList<Machine>();
		machine1.add(new Camera());
		machine1.add(new Camera());
		showMachine(machine1);

		ArrayList<String> list = new ArrayList<String>();

		list.add("One");
		list.add("Two");
		showList(list);
	}

	public static void showMachine1(ArrayList<Machine> machine1) {
		for (Machine mach : machine1) {
			System.out.println(mach);
		}

	}

	public static void showMachine(ArrayList<Machine> machine) {
		for (Machine mach : machine) {
			System.out.println(mach);
		}

	}

	public static void showList(ArrayList<String> list) {
		for (String value : list) {
			System.out.println(value);
		}

	}

}
