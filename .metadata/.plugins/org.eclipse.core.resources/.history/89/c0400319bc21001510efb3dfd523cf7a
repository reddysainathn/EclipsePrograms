package Enums;

//Enum is Enumuration
public class Enums {

	// public static final int DOG = 0;
	// public static final int CAT = 1;
	// public static final int MOUSE = 2;

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// int animal = CAT;
		Animal animal = Animal.DOG;
		switch (animal) {
		case CAT:

			System.out.println("Cat");
			break;
		case DOG:
			System.out.println("Dog");
			break;
		case MOUSE:
			System.out.println("Mouse");
			break;
		default:
			break;

		/*
		 * case DOG: System.out.println("Dog"); break; case CAT:
		 * System.out.println("Cat"); break;
		 */

		}
		System.out.println(Animal.DOG);
		System.out.println(Animal.DOG.getClass());
		//instanceof operator tells whether the Object belongs to particular class of not
		System.out.println(Animal.DOG instanceof Animal );

	}

}
